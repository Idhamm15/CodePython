    "#membuat Fungsi Rekursif untuk Menghitung Bilangan Berpangkat\n",
    "def pangkat(x,y):\n",
    "   if y == 0:\n",
    "      return 1\n",
    "   else:\n",
    "      return x * pangkat(x,y-1)\n",
    "\n",
    "x = int(input(\"Masukan Nilai X : \"))\n",
    "y = int(input(\"Masukan Nilai Y : \"))\n",
    "\n",
    "print(\"%d dipangkatkan %d = %d\" % (x,y,pangkat(x,y)))"
   ]
  }
