    "#Menghitung Bilangan Faktorial Dengan Fungsi Rekursif \n",
    "def faktorial(a):\n",
    "   if a == 1:\n",
    "      return (a)\n",
    "   else:\n",
    "      return (a*faktorial(a-1))\n",
    "\n",
    "bil = int(input(\"Masukan Bilangan : \"))\n",
    "\n",
    "print(\"%d! = %d\" % (bil, faktorial(bil)))"
   ]
  }
