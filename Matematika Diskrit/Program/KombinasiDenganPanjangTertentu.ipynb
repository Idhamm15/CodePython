    "# Program Python untuk mencetak semua \n",
    "# kombinasi dengan panjang tertentu\n",
    "from itertools import combinations \n",
    "\n",
    "# Mendapatkan semua kombinasi dari [1, 2, 3]\n",
    "# dengan panjang 2\n",
    "comb = combinations([1, 2, 3], 2) \n",
    "\n",
    "# Print semua kombinasi\n",
    "for i in comb: \n",
    "    print(i)"
